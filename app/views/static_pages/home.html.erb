        <h1>Campus Events</h1>
        <% @events.each do |event| %>
        <div class="row bottom-space">
          <div class="span1 offset1">
            <div class="circle">
              <span class="event-date"><%=event.sdatetime %><br>10</span>
            </div>
          </div>
          <div class="span9">
            <h4>
            <!%= link_to 'calendar_sync', gsync_path(event) %>
