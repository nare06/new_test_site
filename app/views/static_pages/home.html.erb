            <%= link_to 'Favorites', '#'%>
            <%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %>
            </h4>
            <p><%=event.events_description %>
            </p>
          </div>
        </div>
         <% end %>
        <%= will_paginate @events, class:"pagination pagination-centered" %>
        
