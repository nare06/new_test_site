	<%= stylesheet_link_tag "new/admin" %>
<div class="ft_result">
        <div class="row-fluid">
          <h1 class="page-title">Event Page Preview
          </h1>
                    <ul class="breadcrumb">
            <li> <i class="icon-home"></i> <a href="#">Home</a> <span class="divider">/</span> </li>
