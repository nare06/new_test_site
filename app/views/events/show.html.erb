                    <li class="last">Pending for Moderation</li>
                                     </ul>
                  <hr>
                  <div class="wdcnt" style="width:80%;margin:20px auto;"> 
                   
                    <div class="eventDetail">
                      <div class="eventTitle">
                        <h2><%= @event.title %></h2>
